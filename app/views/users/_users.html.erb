<style>
* {
  text-align: center;
}

</style>

<% @users.each do |user| %>
<tr>
<td><%= user.username %></td>
<td><%= user.password.tr('aA-zZ', '*') %></td>
<td><%= link_to 'Show', user_path(user) %></td>
<td><%= link_to 'Edit', edit_user_path(user) %></td>
<td><%= button_to 'Delete', delete_user_path(user.id), method: :delete %> </td>
<% end %>
</tr>